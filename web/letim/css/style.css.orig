html,body {
      background:  url(../images/bg.png) repeat-x 0 0;
      height: 100%;
      margin: 0; 
      font-family: 'Open Sans', sans-serif;
      font-size: 15px;
      color:#5c6b7b;}
      
body#bg2 {
      background:  url(../images/bg2.jpg) no-repeat 50% 0;}     
      
body#bg3 {
      background:  url(../images/bg3.jpg) no-repeat 50% 0;}      

img {border:none;}      
a{outline: none;color: #004286;}
a:hover {text-decoration:none;}
body,div,h1,h2,h3,h4,h5,h6,input,textarea,p{margin:0; padding:0;} 
input, textarea {outline:none;}	
ul {list-style:disc;list-style-position:outside;margin:7px 0 7px 15px;padding:0;}

.clear {
      margin: 0;
      height: 1px;
      border: none;
      clear:both;
      overflow:hidden;}

#conteiner { 
        min-height: 100%;
        width:1020px;
        margin:0 auto;}
      
* html #conteiner {height: 100%;}

#conteiner_main {padding-bottom: 183px;}

                                                    /* start header */
#header  {
        background: url(../images/top_menu.png) no-repeat 50% 0;
        width: 100%;
        height: 100px;
        margin:0 auto;}
        
#header2  {
        background: url(../images/top_menu.png) no-repeat 50% 0;
        width: 100%;
        height: 130px;
        margin:0 auto;}   
        
#header3  {
        background: url(../images/top_menu.png) no-repeat 50% 0;
        width: 100%;
        height: 113px;
        margin:0 auto;}
        
#header4  {
        background: url(../images/top_menu.png) no-repeat 50% 0;
        width: 100%;
        height: 133px;
        margin:0 auto;}           

#logo {
      position: absolute;
      padding:0 0 0 27px;
      z-index:8;}

ul.top_menu {
       list-style:none;
       padding:0 0 0 180px;
       margin:0;
       float:left;}

ul.top_menu li {
       padding: 0;  
       margin:0px;
       border-right: 1px solid #dedede;
       float:left;}
          

ul.top_menu a {
       float: left;
       height: 67px;
       font-family: 'Open Sans', sans-serif;
       font-size: 14px;
       font-weight: bold;
       color:#346392;
       padding:0 26px;
       margin:0;
       text-transform: uppercase;
       line-height: 67px;
       text-decoration: none;}
       
ul.top_menu a:hover, ul.top_menu a.active {
       height: 61px;
       border-bottom: 6px solid #fc8300;
       line-height: 67px;}
ul.top_menu .active a {
    height: 61px;
    border-bottom: 6px solid #fc8300;
    line-height: 67px;
}
.buton_top {
      float:right;
      padding:15px 0 0 0;}      
       
     
                                                    /* end header */
                                                    
.top_news {
        background: url(../images/top_news.png) no-repeat 0 0;
        position: absolute;
        width: 320px;
        height: 273px;
        padding:43px 0 0 0;
        margin:20px 0 0 34px;
        clear:both;
        overflow:hidden;
        z-index:9999;}

.top_news .news {
      width: 307px;
      padding: 14px 0;
      height:63px;
      margin:0 auto;
      border-bottom:1px solid #e8ebed;
      clear:both;
      overflow:hidden;} 
      
.top_news .news img {
      float:left;
      width: 58px;
      height:58px;
      margin: 0 0 0 20px;}  
      
.top_news .news .text {
      padding:0 0 0 10px;
      overflow:hidden;
	    zoom:1;}  
      
.top_news .news .text p{
      padding:0 0 5px 0;
      font-family: 'Open Sans', sans-serif;
       font-size: 14px;
       color:#767676;}                                                                       
                                                                                                     
.top_news .news .text a{
      background: url(../images/st.png) no-repeat 100% 4px;
      float:right;
      padding:0 20px 0 0;
      margin:0 10px 0 0;
      font-family: 'Open Sans', sans-serif;
       font-size: 14px;
       color:#3794d2;
       text-decoration: none;}  
       
.top_news .news .text a:hover {
       text-decoration: underline;}                
                                                    

#wrapper {
        width:100%;
        margin:5px auto 0 auto;
        clear:both;
      overflow:hidden;}
