<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Letim\CalenderBundle\Entity\MenuItem" table="menu_item">
    <indexes>
      <index name="menu_key" columns="menu_id"/>
      <index name="page_key" columns="page_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false"/>
    <many-to-one field="page" target-entity="Page">
      <join-columns>
        <join-column name="page_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="menu" target-entity="Menu">
      <join-columns>
        <join-column name="menu_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
