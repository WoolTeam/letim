<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Letim\CalenderBundle\Entity\User" table="user">
    <indexes>
      <index name="Roles" columns="userRoles"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="60" nullable="false"/>
    <field name="email" type="string" column="email" length="60" nullable="false"/>
    <field name="pass" type="string" column="pass" length="255" nullable="false"/>
    <field name="salt" type="string" column="salt" length="255" nullable="true"/>
    <many-to-one field="userroles" target-entity="Roles">
      <join-columns>
        <join-column name="userRoles" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
