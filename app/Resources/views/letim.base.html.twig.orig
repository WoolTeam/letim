        <!--start main-->
        <div id="wrapper">

            <!--start left_bar-->
            <div id="left_bar">


                <ul class="left_nav">
                    <li><a href="" title=""><img src="/letim/images/plashka1.png" alt=""/></a></li>
                    <li><a href="" title=""><img src="/letim/images/plashka2.png" alt=""/></a></li>
                    <li><a href="" title=""><img src="/letim/images/plashka3.png" alt=""/></a></li>
                </ul>

                <div class="baner"><a href="" title=""><img src="/letim/images/baner.jpg" alt=""/></a></div>

                <br/>

                <div class="zagolovok">Отзывы</div>
                {% render controller("LetimReviewBundle:Default:list", {'tpl': 'small', 'limit': 2}) %}
            </div>
            <!--end left_bar-->
            <!--start content-->

            {% block content %}
            {% endblock %}
            {% block news %}
                {% if page is defined %}
                    {% render controller("LetimNewsBundle:Default:news",{'id': page.id}) %}
                {% endif %}
            {% endblock %}
            {% block fac %}
                {% if page is defined %}
                    {% render controller("LetimFaqBundle:Default:faq",{'id': page.id}) %}
                {% endif %}
            {% endblock %}
            {#{% include 'LetimNewsBundle:News:news.html.twig' with {'article': article} %}#}
            <!--end content-->

            <!--start bottom_bar-->
            <div id="bottom_bar">
                <div class="form">
                    <form id="sendMail">
                        <div class="error" id="name"></div>
                        <input name="name" type="text" class="pole" value="Ваше имя"
                               onfocus="if(this.value=='Ваше имя'){this.value='';}"
                               onblur="if(this.value==''){this.value='Ваше имя';}"/>
                        <div class="error" id="phone"></div>
                        <input name="phone" type="text" class="pole" value="Ваш телефон"
                               onfocus="if(this.value=='Ваш телефон'){this.value='';}"
                               onblur="if(this.value==''){this.value='Ваш телефон';}"/>
                        <div class="error" id="email"></div>
                        <input name="email" type="text" class="pole" value="Ваш email"
                               onfocus="if(this.value=='Ваш email'){this.value='';}"
                               onblur="if(this.value==''){this.value='Ваш email';}"/>

                        <input name="" class="buton" value="Забронировать полет" type="submit"/>
                    </form>
                </div>


                <div class="map">
                    <img src="/letim/images/map.jpg" alt=""/>
                </div>
            </div>
            <!--end bottom_bar-->
        </div>
        <!--end main-->
    </div>
